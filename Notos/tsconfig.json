{
  "compilerOptions": {
    "outDir": "./resources/js",
    "removeComments": true,
    "target": "ES2022",
    "moduleResolution": "node",
    "alwaysStrict": true,
    "allowUnreachableCode": false,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": [
    "./src/*.ts"
  ]
}